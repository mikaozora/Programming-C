//1
input panjang
input lebar
set luas = panjang * lebar
print luas

//2
input seconds
set minutes = seconds / 60
set hours = seconds / 3600
print minutes
print hours

//3
input whether
IF whether % 2 == 0
    print "whether is even"
Else
    print "whether is odd"
Endif 

//4
input r
set phi = 3.14
set luas = phi * r * r
print luas

//5
input number1
input number2
input number3
if number1 > number2 AND number1 > number3
    print "number 1 is the largest"
else if number2 > number1 AND number2 > number3
    print "number 2 is the largest"
else
    print "number 3 is the largest"
Endif